<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.github.talelin.latticy.mapper.SymptomMapper">

    <resultMap id="BaseResultMap" type="io.github.talelin.latticy.model.SymptomDO">
        <id column="id"  property="id"/>
        <result column="symptom"  property="symptom"/>
    </resultMap>


    <select id="getSymptoms" resultMap="BaseResultMap">
     SELECT * FROM t_symptom;
    </select>
</mapper>